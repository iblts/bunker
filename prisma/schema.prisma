generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  id         Int     @id @default(autoincrement())
  name       String?
  sex        String?
  age        String?
  height     String?
  profession String?
  health     String?
  hobby      String?
  phobia     String?
  character  String?
  inventory  String?
  extra      String?
}

model Data {
  id      Int     @id @default(autoincrement())
  place   String?
  size    String?
  time    String?
  food    String?
  rooms   String?
  problem String?
}
